<div class="footer__container">
  <div class="row">
    <% Contact.all.each do |contact| %>
      <div class="col-sm-12 col-lg-3">
        <%= render partial: 'contacts/contact', locals: { contact: contact } %>
      </div>
    <% end %>
    <div class="col-sm-12 col-lg-3">
      <%= render partial: 'languages/language' %>
    </div>
    <div class="col-sm-12 col-lg-3">
      <%= render partial: 'links/link' %>
    </div>
  </div>
</div>
