<div class="contacts__card footer__card">
  <div class="contacts__content footer__content">
    <div class="card__title">
      <p>Contacts <%= contact.name %></p>
      <p class="card__title--underline"></p>
    </div>
    <div class="card__info">
      <p><%= contact.address %></p>
      <p><%= contact.phone_number %></p>
      <p><%= contact.email %></p>
      <p><%= contact.schedule %></p>
    </div>
  </div>
</div>
